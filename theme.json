{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "blocks": {},
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        },
        "color": {
            "background": true,
            "custom": false,
            "customDuotone": true,
            "customGradient": true,
            "defaultDuotone": true,
            "defaultGradients": true,
            "defaultPalette": false,
            "link": true,
            "text": true,
            "duotone": [
                {
                    "name": "Black to White",
                    "colors": [ "#000", "#FFF" ],
                    "slug": "black-to-white"
                }
            ],
            "gradients": [
                {
                    "name": "Black to white",
                    "gradient": "linear-gradient(135deg, var(--wp--preset--color--foreground), var(--wp--preset--color--pure-white))",
                    "slug": "black-to-white"
                },
                {
                    "name": "Primary to Secondary",
                    "gradient": "linear-gradient(160deg, var(--wp--preset--color--primary), var(--wp--preset--color--secondary))",
                    "slug": "primary-to-secondary"
                }
            ],
            "palette": [
                {
                    "name": "Off White",
                    "color": "#f7f7f7",
                    "slug": "background"
                },
                {
                    "name": "Lighter Grey",
                    "color": "#eaeaea",
                    "slug": "greyscale-primary"
                },
                {
                    "name": "Light Grey",
                    "color": "#bfbfbf",
                    "slug": "greyscale-secondary"
                },
                {
					"name": "Mid Grey",
					"color": "#959595",
                    "slug": "greyscale-tertiary"
				},
                {
                    "name": "Dark Grey",
                    "color": "#6a6a6a",
                    "slug": "greyscale-quaternary"
                },
                {
					"name": "Darker Grey",
                    "color": "#404040",
					"slug": "greyscale-quinary"
				},
                {
                    "name": "Base Black",
                    "color": "#151515",
                    "slug": "foreground"
                },
                {
					"name": "Brand Primary",
					"slug": "primary",
					"color": "#6f7e9a"
				},
                {
					"name": "Brand Primary (Light)",
					"slug": "primary-light",
					"color": "#9eadcb"
				},
                {
					"name": "Brand Primary (Dark)",
					"slug": "primary-dark",
					"color": "#43526c"
				},
				{
					"name": "Brand Secondary",
					"slug": "secondary",
					"color": "#454d33"
				},
                {
					"name": "Brand Secondary (Light)",
					"slug": "secondary-light",
					"color": "#71795d"
				},
                {
					"name": "Brand Secondary (Dark)",
					"slug": "secondary-dark",
					"color": "#1e250c"
				},
                {
                    "name": "Link Blue",
                    "color": "#0820A1",
                    "slug": "link-primary"
                },
                {
                    "name": "Link Light Blue",
                    "color": "#564ad3",
                    "slug": "link-secondary"
                },
                {
                    "name": "Link Dark Blue",
                    "color": "#000071",
                    "slug": "link-tertiary"
                }
            ]
        },
        "custom": {
            "border": {
                "radius": {
                    "sm": "0.25rem",
                    "md": "0.5rem",
                    "lg": "0.75rem",
                    "rounded": "99rem"
                }
            },
            "font-weight": {
                "light": "200",
                "base": "400",
                "medium": "700",
                "heavy": "800"
            },
            "line-height": {
                "base": 1.125,
                "tiny": "calc(var(--wp--custom--line-height--base) / var(--wp--custom--spacing--scale-sm))",
                "small": "var(--wp--custom--line-height--base)",
                "normal": "calc(var(--wp--custom--line-height--base) * var(--wp--custom--spacing--scale-sm))",
                "medium": "calc(var(--wp--custom--line-height--base) * var(--wp--custom--spacing--scale-sm-squared))",
                "huge": "calc(var(--wp--custom--line-height--base) * var(--wp--custom--spacing--scale-sm-cubed))"
            },
            "spacing": {
                "scale-sm": 1.25,
                "scale-sm-squared": "calc(var(--wp--custom--spacing--scale-sm) * var(--wp--custom--spacing--scale-sm))",
                "scale-sm-cubed": "calc(var(--wp--custom--spacing--scale-sm-squared) * var(--wp--custom--spacing--scale-sm))",
                "scale-lg": 1.333,
                "scale-lg-squared": "calc(var(--wp--custom--spacing--scale-lg) * var(--wp--custom--spacing--scale-lg))",
                "scale-lg-cubed": "calc(var(--wp--custom--spacing--scale-lg-squared) * var(--wp--custom--spacing--scale-lg))"
			}
        },
        "layout": {
            "contentSize": "48rem",
            "wideSize": "65rem"
        },
        "spacing": {
            "blockGap": true,
            "customSpacingSize": true,
            "margin": false,
            "padding": false,
            "spacingScale": {},
            "spacingSizes": [],
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "%"
            ]
        },
        "typography": {
            "customFontSize": false,
            "dropCap": false,
            "fontStyle": true,
            "fontWeight": true,
            "letterSpacing": true,
            "lineHeight": false,
            "textDecoration": true,
            "textTransform": true,
            "fontSizes": [
                {
					"name": "Tiny",
					"size": "var(--step--2)",
					"slug": "tiny"
				},
                {
					"name": "Small",
					"size": "var(--step--1)",
					"slug": "small"
				},
                {
					"name": "Normal",
					"size": "var(--step-0)",
					"slug": "normal"
				},
				{
					"name": "Medium",
					"size": "var(--step-1)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "var(--step-2)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "var(--step-3)",
					"slug": "x-large"
				},
                {
					"name": "Huge",
					"size": "var(--step-4)",
					"slug": "huge"
				},
                {
					"name": "Gigantic",
					"size": "var(--step-5)",
					"slug": "gigantic"
				}
            ],
            "fontFamilies": [
                {
                    "name": "Sans Serif",
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Helvetica Neue', Helvetica, 'Avenir Next', Avenir, 'Segoe UI', Cantarell, Ubuntu, Roboto, Oxygen-Sans, Noto, Arial, Geneva, Tahoma, Verdana, sans-serif",
					"slug": "sans-serif"
				},
                {
                    "name": "Serif",
                    "fontFamily": "'Iowan Old Style', 'Apple Garamond', Baskerville, 'Droid Serif', 'Source Serif Pro', 'Times New Roman', 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol', Cambria, Georgia, Times, serif",
			        "slug": "serif"
                },
                {
                    "name": "Mono",
                    "fontFamily": "Menlo, Consolas, Monaco, Liberation Mono, Lucida Console, monospace;",
			        "slug": "mono"
                }
            ]
        }
    },
    "styles": {
        "blocks": {
            "contact-form-7/contact-form-selector": {
                "spacing": {
                    "padding": {
                        "left": "var(--space-xs-4xs)",
                        "right": "var(--space-xs-4xs)"
                    }
                }
            },
            "core/buttons": {
                "spacing": {
                    "padding": {
                        "left": "var(--space-xs-4xs)",
                        "right": "var(--space-xs-4xs)"
                    }
                }
            },
            "core/code": {
                "spacing": {
                    "padding": {
                        "left": "var(--space-xs)",
                        "right": "var(--space-xs)"
                    }
                }
            },
            "core/cover": {
                "spacing": {
                    "padding": {
                        "top": "var(--space-s-m)",
                        "right": "var(--space-s-m)",
                        "bottom": "var(--space-s-m)",
                        "left": "var(--space-s-m)"
                    }
                }
            },
            "core/heading": {
                "spacing": {
                    "padding": {
                        "left": "var(--space-xs)",
                        "right": "var(--space-xs)"
                    }
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--serif)"
                }
            },
            "core/list": {
                "spacing": {
                    "padding": {
                        "right": "var(--space-xs)",
                        "left": "var(--space-l-xl)"
                    }
                }
            },
            "core/paragraph": {
                "spacing": {
                    "padding": {
                        "left": "var(--space-xs)",
                        "right": "var(--space-xs)"
                    }
                }
            },
            "core/post-date": {
                "typography": {
                    "fontSize": "var(--wp--custom--font-weight--medium)"
                },
                "spacing": {
                    "margin": {
                        "top": "auto"
                    },
                    "padding": {
                        "left": "var(--space-xs)",
                        "right": "var(--space-xs)"
                    }
                },
                "color": {
                    "text": "var(--wp--preset--color--greyscale-quaternary)"
                }
            },
            "core/post-excerpt": {
				"elements": {
					"link": {
                        "border": {
                            "radius": "4px"
                        },
						"color": {
				            "background": "var(--wp--preset--color--secondary)",
					        "text": "var(--wp--preset--color--background)"
						},
						"spacing": {
							"padding": {
								"top": "calc(1rem * (var(--wp--custom--spacing--scale-lg) / 2) + 2px)",
								"right": "calc(1em * var(--wp--custom--spacing--scale-lg-cubed) + 2px)",
								"bottom": "calc(1rem * (var(--wp--custom--spacing--scale-lg) / 2) + 2px)",
								"left": "calc(1em * var(--wp--custom--spacing--scale-lg-cubed) + 2px)"
							}
						},
                        "typography": {
                            "fontWeight": "var(--wp--custom--font-weight--medium)",
                            "fontSize": "var(--wp--preset--font-size--normal)",
                            "textDecoration": "none"
                        }
					}
				},
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)"
                }
			},
            "core/post-terms": {
                "spacing": {
                    "padding": {
                        "left": "var(--space-xs)"
                    }
                }
            },
            "core/preformatted": {
                "spacing": {
                    "padding": {
                        "left": "var(--space-xs)",
                        "right": "var(--space-xs)"
                    }
                }
            },
            "core/pullquote": {
                "color": {
                    "background": "var(--wp--preset--color--greyscale-primary)"
                },
                "spacing": {
                    "padding": {
                        "left": "var(--space-m)",
                        "right": "var(--space-m)"
                    }
                }
            },
            "core/quote": {
                "border": {
                    "style": "solid",
                    "color": "var(--wp--preset--color--greyscale-quaternary)",
                    "width": "0 0 0 5px"
                },
                "color": {
                    "text": "var(--wp--preset--color--greyscale-quinary)",
                    "background": "antiquewhite"
                },
                "spacing": {
                    "padding": {
                        "top": "var(--space-2xs)",
                        "right": "var(--space-s-m)",
                        "bottom": "var(--space-l)",
                        "left": "var(--space-xs-s)"
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)"
                }
            },
            "core/search": {
                "color": {
                    "background": "var(--wp--preset--color--greyscale-primary)"
                },
                "spacing": {
                    "padding": {
                        "top": "var(--space-xs-s)",
                        "right": "var(--space-xs-s)",
                        "bottom": "var(--space-xs-s)",
                        "left": "var(--space-xs-s)"
                    }
                }
            },
            "core/site-title": {
                "border": {},
                "color": {},
                "elements": {},
                "spacing": {},
                "typography": {}
            },
            "core/table": {
                "spacing": {
                    "margin": {
                        "top": "var(--space-xs-s)",
                        "bottom": "var(--space-xs-s)"
                    }
                }
            }
        },
        "border": {},
        "color": {
            "text": "var(--wp--preset--color--foreground)",
            "background": "var(--wp--preset--color--background)"
        },
        "elements": {
            "link": {
                "border": {},
                "color": {
                    "background": "transparent",
                    "text": "var(--wp--preset--color--link-primary)"
                },
                "spacing": {},
                "typography": {}
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--huge)",
                    "fontWeight": "var(--wp--custom--font-weight--medium)",
                    "lineHeight": "var(--wp--custom--line-height--small)"
                }
            },
            "h2": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--x-large)",
                    "fontWeight": "var(--wp--custom--font-weight--medium)",
                    "lineHeight": "var(--wp--custom--line-height--normal)"
                }
            },
            "h3": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--large)",
                    "fontWeight": "var(--wp--custom--font-weight--medium)",
                    "lineHeight": "var(--wp--custom--line-height--normal)"
                }
            },
            "h4": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "var(--wp--custom--font-weight--medium)",
                    "lineHeight": "var(--wp--custom--line-height--normal)"
                }
            },
            "h5": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "500",
                    "lineHeight": "var(--wp--custom--line-height--normal)"
                },
                "color": {
                    "text": "var(--wp--preset--color--greyscale-quinary)"
                }
            },
            "h6": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "var(--wp--custom--font-weight--medium)",
                    "lineHeight": "var(--wp--custom--line-height--medium)",
                    "textTransform": "uppercase"
                },
                "color": {
                    "text": "var(--wp--preset--color--greyscale-quaternary)"
                }
            }
        },
        "spacing": {
            "blockGap": "var(--space-xs-s)"
        },
        "typography": {
			"fontFamily": "var(--wp--preset--font-family--sans-serif)",
            "fontSize": "var(--wp--preset--font-size--normal)",
            "fontWeight": "var(--wp--custom--font-weight--light)",
			"lineHeight": "var(--wp--custom--line-height--normal)"
        }
    },
    "customTemplates": [
        {
            "title": "Page with empty content area",
            "name": "page-empty",
            "postTypes": [
                "page",
                "post"
            ]
        }
    ],
    "patterns": [
        "partner-logos"
    ],
    "templateParts": [
        {
            "title": "Header",
            "name": "header",
            "area": "header"
        },
        {
            "title": "Footer",
            "name": "footer",
            "area": "footer"
        }
    ]
}
