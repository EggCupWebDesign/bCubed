{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "blocks": {},
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        },
        "color": {
            "background": true,
            "custom": false,
            "customDuotone": true,
            "customGradient": true,
            "defaultDuotone": true,
            "defaultGradients": true,
            "defaultPalette": false,
            "link": true,
            "text": true,
            "duotone": [
                {
                    "name": "Black to White",
                    "colors": [ "#000", "#FFF" ],
                    "slug": "black-to-white"
                }
            ],
            "gradients": [
                {
                    "name": "Black to white",
                    "gradient": "linear-gradient(135deg, var(--wp--preset--color--foreground), var(--wp--preset--color--pure-white))",
                    "slug": "black-to-white"
                },
                {
                    "name": "Primary to Secondary",
                    "gradient": "linear-gradient(160deg, var(--wp--preset--color--primary), var(--wp--preset--color--secondary))",
                    "slug": "primary-to-secondary"
                }
            ],
            "palette": [
                {
                    "name": "Off White",
                    "color": "#f7f7f7",
                    "slug": "background"
                },
                {
                    "name": "Lighter Grey",
                    "color": "#eaeaea",
                    "slug": "greyscale-primary"
                },
                {
                    "name": "Light Grey",
                    "color": "#bfbfbf",
                    "slug": "greyscale-secondary"
                },
                {
					"name": "Mid Grey",
					"color": "#959595",
                    "slug": "greyscale-tertiary"
				},
                {
                    "name": "Dark Grey",
                    "color": "#6a6a6a",
                    "slug": "greyscale-quaternary"
                },
                {
					"name": "Darker Grey",
                    "color": "#404040",
					"slug": "greyscale-quinary"
				},
                {
                    "name": "Base Black",
                    "color": "#151515",
                    "slug": "foreground"
                },
                {
					"name": "Brand Primary",
					"slug": "primary",
					"color": "#6f7e9a"
				},
                {
					"name": "Brand Primary (Light)",
					"slug": "primary-light",
					"color": "#9eadcb"
				},
                {
					"name": "Brand Primary (Dark)",
					"slug": "primary-dark",
					"color": "#43526c"
				},
				{
					"name": "Brand Secondary",
					"slug": "secondary",
					"color": "#454d33"
				},
                {
					"name": "Brand Secondary (Light)",
					"slug": "secondary-light",
					"color": "#71795d"
				},
                {
					"name": "Brand Secondary (Dark)",
					"slug": "secondary-dark",
					"color": "#1e250c"
				},
                {
                    "name": "Link Blue",
                    "color": "#0820A1",
                    "slug": "link-primary"
                },
                {
                    "name": "Link Light Blue",
                    "color": "#564ad3",
                    "slug": "link-secondary"
                },
                {
                    "name": "Link Dark Blue",
                    "color": "#000071",
                    "slug": "link-tertiary"
                }
            ]
        },
        "custom": {
            "border": {
                "radius": {
                    "sm": "0.25rem",
                    "md": "0.5rem",
                    "lg": "0.75rem",
                    "rounded": "99rem"
                }
            },
            "font-weight": {
                "light": "200",
                "base": "400",
                "medium": "700",
                "heavy": "800"
            },
            "line-height": {
                "base": 1.35,
                "tiny": "var(--space-xs-s)",
                "small": "var(--space-s)",
                "normal": "var(--space-m)",
                "medium": "var(--space-m-l)",
                "large": "var(--space-xl-2xl)"
            }
        },
        "layout": {
            "contentSize": "50rem",
            "wideSize": "80rem"
        },
        "spacing": {
            "blockGap": true,
            "customSpacingSize": false,
            "spacingScale": {},
			"spacingSizes": [
				{
					"size": "var(--space-3xs)",
					"slug": "10",
					"name": "1"
				},
				{
					"size": "var(--space-2xs)",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "var(--space-xs)",
					"slug": "30",
					"name": "3"
				},
				{
					"size": "var(--space-s)",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "var(--space-m)",
					"slug": "50",
					"name": "5"
				},
				{
					"size": "var(--space-l)",
					"slug": "60",
					"name": "6"
				},
				{
					"size": "var(--space-xl)",
					"slug": "70",
					"name": "7"
				},
				{
					"size": "var(--space-2xl)",
					"slug": "80",
					"name": "8"
				},
				{
					"size": "var(--space-3xl)",
					"slug": "90",
					"name": "9"
				}
			],
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "%"
            ]
        },
        "useRootPaddingAwareAlignments": true,
        "typography": {
            "customFontSize": false,
            "dropCap": false,
            "fluid": true,
            "fontStyle": true,
            "fontWeight": true,
            "letterSpacing": true,
            "lineHeight": true,
            "textDecoration": true,
            "textTransform": true,
            "fontSizes": [
                {
					"name": "Tiny",
                    "fluid": {
                        "min": "0.84rem",
                        "max": "0.87rem"
                    },
					"size": "0.85rem",
					"slug": "tiny"
				},
                {
					"name": "Small",
                    "fluid": {
                        "min": "1.04rem",
                        "max": "1.13rem"
                    },
					"size": "1.085rem",
					"slug": "small"
				},
                {
					"name": "Normal",
                    "fluid": {
                        "min": "1.25rem",
                        "max": "1.50rem"
                    },
					"size": "1.625rem",
					"slug": "normal"
				},
				{
					"name": "Medium",
                    "fluid": {
                        "min": "1.50rem",
                        "max": "2.00rem"
                    },
					"size": "1.75rem",
					"slug": "medium"
				},
				{
					"name": "Large",
                    "fluid": {
                        "min": "1.80rem",
                        "max": "2.67rem"
                    },
					"size": "2.235rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
                    "fluid": {
                        "min": "2.16rem",
                        "max": "3.55rem"
                    },
					"size": "2.855rem",
					"slug": "x-large"
				},
                {
					"name": "Huge",
                    "fluid": {
                        "min": "2.59rem",
                        "max": "4.74rem"
                    },
					"size": "3.665rem",
					"slug": "huge"
				},
                {
					"name": "Gigantic",
                    "fluid": {
                        "min": "3.11rem",
                        "max": "6.31rem"
                    },
					"size": "4.725rem",
					"slug": "gigantic"
				}
            ],
            "fontFamilies": [
                {
					"name": "Roboto Slab",
                    "fontFamily": "\"Roboto Slab\", serif",
					"slug": "heading-slab",
                    "fontFace": [
                        {
                            "fontFamily": "\"Roboto Slab\"",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "400",
                            "src": [
                                "file:./dist/fonts/roboto-slab-v25-latin-regular.woff2"
                            ]
                        },
                        {
                            "fontFamily": "\"Roboto Slab\"",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "700",
                            "src": [
                                "file:./dist/fonts/roboto-slab-v25-latin-700.woff2"
                            ]
                        }
                    ]
                },
                {
                    "name": "Sans Serif",
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Helvetica Neue', Helvetica, 'Avenir Next', Avenir, 'Segoe UI', Cantarell, Ubuntu, Roboto, Oxygen-Sans, Noto, Arial, Geneva, Tahoma, Verdana, sans-serif",
					"slug": "sans-serif"
				},
                {
                    "name": "Serif",
                    "fontFamily": "'Iowan Old Style', 'Apple Garamond', Baskerville, 'Droid Serif', 'Source Serif Pro', 'Times New Roman', 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol', Cambria, Georgia, Times, serif",
			        "slug": "serif"
                },
                {
                    "name": "Mono",
                    "fontFamily": "Menlo, Consolas, Monaco, Liberation Mono, Lucida Console, monospace;",
			        "slug": "mono"
                }
            ]
        }
    },
    "styles": {
        "blocks": {
            "contact-form-7/contact-form-selector": {
                "spacing": {
                    "padding": {
                        "left": "var(--space-xs-4xs)",
                        "right": "var(--space-xs-4xs)"
                    }
                }
            },
            "core/button": {
                "spacing": {
                    "padding": {
                        "top": "var(--space-2xs-xs)",
                        "right": "var(--space-s-m)",
                        "bottom": "var(--space-2xs-xs)",
                        "left": "var(--space-s-m)"
                    }
                }
            },
            "core/code": {
                "spacing": {
                    "padding": {
                        "left": "var(--space-xs)",
                        "right": "var(--space-xs)"
                    }
                }
            },
            "core/cover": {
                "spacing": {
                    "padding": {
                        "top": "var(--space-s-m)",
                        "right": "var(--space-s-m)",
                        "bottom": "var(--space-s-m)",
                        "left": "var(--space-s-m)"
                    }
                }
            },
            "core/navigation": {
				"elements": {
					"link": {
						":hover": {
                            "color": {
                                "background": "var(--wp--preset--color--primary)",
                                "text": "var(--wp--preset--color--background) !important"
                            }
						},
						":focus": {
							"typography": {
								"textDecoration": "underline dashed"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
                "spacing": {
                    "blockGap": "var(--space-4xs-3xs)"
                },
                "typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
                    "lineHeight": "var(--wp--custom--line-height--small)"
				}
            },
            "core/navigation-link": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-slab)"
                }
            },
            "core/navigation-submenu": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-slab)",
                    "lineHeight": "var(--wp--custom--line-height--base)"
                }
            },
            "core/post-author": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-slab)",
                    "lineHeight": "var(--wp--custom--line-height--tiny)"
                }
            },
            "core/post-author-name": {
                "typography": {
                    "fontWeight": "var(--wp--custom--font-weight--base)"
                }
            },
            "core/post-date": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-slab)",
                    "fontWeight": "var(--wp--custom--font-weight--base)",
                    "lineHeight": "var(--wp--custom--line-height--base)"
                }
            },
            "core/post-excerpt": {
				"elements": {
					"link": {
                        "border": {
                            "radius": "var(--wp--custom--border--radius--sm)"
                        },
						"color": {
				            "background": "var(--wp--preset--color--secondary)",
					        "text": "var(--wp--preset--color--background)"
						},
                        ":hover": {
                            "color": {
                                "background": "var(--wp--preset--color--secondary-dark)",
                                "text": "var(--wp--preset--color--background)"
                            }
                        },
                        ":focus": {
                            "color": {
                                "background": "var(--wp--preset--color--secondary-dark)",
                                "text": "var(--wp--preset--color--background)"
                            }
                        },
                        ":active": {
                            "color": {
                                "background": "var(--wp--preset--color--secondary)",
                                "text": "var(--wp--preset--color--background)"
                            }
                        },
						"spacing": {
                            "padding": {
                                "top": "var(--space-2xs-xs)",
                                "right": "var(--space-s-m)",
                                "bottom": "var(--space-2xs-xs)",
                                "left": "var(--space-s-m)"
                            }
                        },
                        "typography": {
                            "fontWeight": "var(--wp--custom--font-weight--medium)",
                            "fontSize": "var(--wp--preset--font-size--normal)",
                            "lineHeight": "var(--wp--custom--line-height--small)",
                            "textDecoration": "none"
                        }
					}
				},
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "lineHeight": "var(--wp--custom--line-height--medium)"
                }
			},
            "core/post-navigation-link": {
                "typography": {
                  "fontSize": "var(--wp--preset--font-size--small)",
                  "fontWeight": "var(--wp--custom--font-weight--medium)"
                }
              },
            "core/post-terms": {
                "spacing": {
                    "padding": {
                        "left": "var(--space-xs)"
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)"
                }
            },
            "core/preformatted": {
                "spacing": {
                    "padding": {
                        "left": "var(--space-xs)",
                        "right": "var(--space-xs)"
                    }
                }
            },
            "core/pullquote": {
                "color": {
                    "background": "var(--wp--preset--color--greyscale-primary)"
                },
                "spacing": {
                    "padding": {
                        "left": "var(--space-m)",
                        "right": "var(--space-m)"
                    }
                }
            },
            "core/query-pagination": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "var(--wp--custom--font-weight--medium)"
                  }
            },
            "core/quote": {
                "border": {
                    "style": "solid",
                    "color": "var(--wp--preset--color--greyscale-quaternary)",
                    "width": "0 0 0 5px"
                },
                "color": {
                    "text": "var(--wp--preset--color--greyscale-quinary)",
                    "background": "antiquewhite"
                },
                "spacing": {
                    "padding": {
                        "top": "var(--space-2xs)",
                        "right": "var(--space-s-m)",
                        "bottom": "var(--space-l)",
                        "left": "var(--space-xs-s)"
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)"
                }
            },
            "core/search": {
                "color": {
                    "background": "var(--wp--preset--color--greyscale-primary)"
                },
                "spacing": {
                    "padding": {
                        "top": "var(--space-xs-s)",
                        "right": "var(--space-xs-s)",
                        "bottom": "var(--space-xs-s)",
                        "left": "var(--space-xs-s)"
                    }
                }
            },
            "core/site-title": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                },
                "typography": {
                    "lineHeight": "var(--wp--custom--line-height--base)"
                }
            },
            "core/table": {
                "spacing": {
                    "margin": {
                        "top": "var(--space-xs-s)",
                        "bottom": "var(--space-xs-s)"
                    }
                }
            }
        },
        "border": {},
        "color": {
            "text": "var(--wp--preset--color--foreground)",
            "background": "var(--wp--preset--color--background)"
        },
        "elements": {
            "button": {
				"border": {
					"radius": "var(--wp--custom--border--radius--sm)"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--background)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--secondary-dark)",
						"text": "var(--wp--preset--color--background)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--secondary-dark)",
						"text": "var(--wp--preset--color--background)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--background)"
					}
				},
                "typography": {
                    "fontWeight": "var(--wp--custom--font-weight--heavy)"
                }
			},
            "link": {
                "border": {},
                "color": {
                    "background": "transparent",
                    "text": "var(--wp--preset--color--link-primary)"
                },
                ":hover": {
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"typography": {
						"textDecoration": "underline dashed"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--link-tertiary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
                "spacing": {},
                "typography": {
					"textDecoration": "underline"
				}
            },
            "heading": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-slab)",
                    "lineHeight": "var(--wp--custom--line-height--base)"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--huge)",
                    "fontWeight": "var(--wp--custom--font-weight--medium)",
                    "lineHeight": "var(--wp--custom--line-height--large)"
                }
            },
            "h2": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--x-large)",
                    "fontWeight": "var(--wp--custom--font-weight--medium)"
                }
            },
            "h3": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--large)",
                    "fontWeight": "var(--wp--custom--font-weight--base)"
                }
            },
            "h4": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "var(--wp--custom--font-weight--base)"
                }
            },
            "h5": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "var(--wp--custom--font-weight--base)"
                },
                "color": {
                    "text": "var(--wp--preset--color--greyscale-quinary)"
                }
            },
            "h6": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "var(--wp--custom--font-weight--light)",
                    "lineHeight": "var(--wp--custom--line-height--medium)",
                    "textTransform": "uppercase"
                },
                "color": {
                    "text": "var(--wp--preset--color--greyscale-quaternary)"
                }
            }
        },
        "spacing": {
            "blockGap": "var(--space-s-m)",
            "padding": {
				"right": "var(--wp--preset--spacing--40)",
				"left": "var(--wp--preset--spacing--40)"
			}
        },
        "typography": {
			"fontFamily": "var(--wp--preset--font-family--sans-serif)",
            "fontSize": "var(--wp--preset--font-size--normal)",
            "fontWeight": "var(--wp--custom--font-weight--light)",
			"lineHeight": "var(--wp--custom--line-height--normal)"
        }
    },
    "customTemplates": [
        {
            "title": "Page without title",
            "name": "page-empty",
            "postTypes": [
                "page",
                "post"
            ]
        },
        {
            "title": "Page with hero section",
            "name": "page-hero",
            "postTypes": [
                "page",
                "post"
            ]
        },
        {
            "title": "Post with hero featured image",
            "name": "single-hero",
            "postTypes": [
                "post"
            ]
        }
    ],
    "patterns": [
        "partner-logos"
    ],
    "templateParts": [
        {
            "title": "Footer",
            "name": "footer",
            "area": "footer"
        },
        {
            "title": "Header",
            "name": "header",
            "area": "header"
        },
        {
            "title": "Post Meta",
            "name": "post-meta",
            "area": "uncategorized"
        },
        {
            "title": "Post Navigation",
            "name": "post-navigation",
            "area": "uncategorized"
        },
        {
            "title": "Post Terms",
            "name": "post-terms",
            "area": "uncategorized"
        },
        {
            "title": "Query Pagination",
            "name": "query-pagination",
            "area": "uncategorized"
        }
    ]
}
