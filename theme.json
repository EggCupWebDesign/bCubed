{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "blocks": {},
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        },
        "color": {
            "background": true,
            "button": true,
            "custom": false,
            "customDuotone": true,
            "customGradient": true,
            "defaultDuotone": true,
            "defaultGradients": true,
            "defaultPalette": false,
            "heading": true,
            "link": true,
            "text": true,
            "duotone": [
                {
                    "name": "Black to White",
                    "colors": [ "#000", "#FFF" ],
                    "slug": "black-to-white"
                }
            ],
            "gradients": [
                {
                    "name": "Black to white",
                    "gradient": "linear-gradient(135deg, var(--wp--preset--color--foreground), var(--wp--preset--color--white))",
                    "slug": "black-to-white"
                },
                {
                    "name": "Primary to Secondary",
                    "gradient": "linear-gradient(160deg, var(--wp--preset--color--primary), var(--wp--preset--color--secondary))",
                    "slug": "primary-to-secondary"
                }
            ],
            "palette": [
                {
                    "name": "Transparent",
                    "slug": "transparent",
                    "color": "transparent"
                },
                {
                    "name": "Off White",
                    "color": "#f7f7f7",
                    "slug": "background"
                },
                {
                    "name": "Lighter Grey",
                    "color": "#eaeaea",
                    "slug": "greyscale-primary"
                },
                {
                    "name": "Light Grey",
                    "color": "#bfbfbf",
                    "slug": "greyscale-secondary"
                },
                {
					"name": "Mid Grey",
					"color": "#959595",
                    "slug": "greyscale-tertiary"
				},
                {
                    "name": "Dark Grey",
                    "color": "#6a6a6a",
                    "slug": "greyscale-quaternary"
                },
                {
					"name": "Darker Grey",
                    "color": "#404040",
					"slug": "greyscale-quinary"
				},
                {
                    "name": "Base Black",
                    "color": "#151515",
                    "slug": "foreground"
                },
                {
					"name": "Brand Primary",
					"slug": "primary",
					"color": "#6f7e9a"
				},
                {
					"name": "Brand Primary (Light)",
					"slug": "primary-light",
					"color": "#9eadcb"
				},
                {
					"name": "Brand Primary (Dark)",
					"slug": "primary-dark",
					"color": "#43526c"
				},
				{
					"name": "Brand Secondary",
					"slug": "secondary",
					"color": "#454d33"
				},
                {
					"name": "Brand Secondary (Light)",
					"slug": "secondary-light",
					"color": "#71795d"
				},
                {
					"name": "Brand Secondary (Dark)",
					"slug": "secondary-dark",
					"color": "#1e250c"
				},
                {
                    "name": "Link Primary",
                    "color": "#0820A1",
                    "slug": "link-primary"
                },
                {
                    "name": "Link Secondary",
                    "color": "#564ad3",
                    "slug": "link-secondary"
                },
                {
                    "name": "Link Tertiary",
                    "color": "#000071",
                    "slug": "link-tertiary"
                }
            ]
        },
        "custom": {
            "border": {
                "radius": {
                    "sm": "0.25rem",
                    "md": "0.5rem",
                    "lg": "0.75rem",
                    "rounded": "99rem"
                }
            },
            "font-weight": {
                "light": "200",
                "base": "400",
                "medium": "700",
                "heavy": "800"
            },
            "line-height": {
                "base": 1.333,
                "tiny": 1.067,
                "small": 1.125,
                "normal": 1.25,
                "medium": 1.414,
                "large": 1.5
            }
        },
        "layout": {
            "contentSize": "50rem",
            "wideSize": "80rem"
        },
        "spacing": {
            "blockGap": true,
            "customSpacingSize": false,
            "spacingScale": {},
			"spacingSizes": [
				{
					"size": "var(--space-3xs)",
					"slug": "10",
					"name": "1"
				},
				{
					"size": "var(--space-2xs)",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "var(--space-xs)",
					"slug": "30",
					"name": "3"
				},
				{
					"size": "var(--space-s)",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "var(--space-m)",
					"slug": "50",
					"name": "5"
				},
				{
					"size": "var(--space-l)",
					"slug": "60",
					"name": "6"
				},
				{
					"size": "var(--space-xl)",
					"slug": "70",
					"name": "7"
				},
				{
					"size": "var(--space-2xl)",
					"slug": "80",
					"name": "8"
				},
				{
					"size": "var(--space-3xl)",
					"slug": "90",
					"name": "9"
				}
			],
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw",
                "%"
            ]
        },
        "useRootPaddingAwareAlignments": true,
        "typography": {
            "customFontSize": false,
            "dropCap": false,
            "fluid": true,
            "fontStyle": true,
            "fontWeight": true,
            "letterSpacing": true,
            "lineHeight": true,
            "textDecoration": true,
            "textTransform": true,
            "fontSizes": [
                {
					"name": "Tiny",
                    "fluid": {
                        "min": "0.84rem",
                        "max": "0.87rem"
                    },
					"size": "0.85rem",
					"slug": "tiny"
				},
                {
					"name": "Small",
                    "fluid": {
                        "min": "1.04rem",
                        "max": "1.13rem"
                    },
					"size": "1.085rem",
					"slug": "small"
				},
                {
					"name": "Normal",
                    "fluid": {
                        "min": "1.25rem",
                        "max": "1.50rem"
                    },
					"size": "1.625rem",
					"slug": "normal"
				},
				{
					"name": "Medium",
                    "fluid": {
                        "min": "1.50rem",
                        "max": "2.00rem"
                    },
					"size": "1.75rem",
					"slug": "medium"
				},
				{
					"name": "Large",
                    "fluid": {
                        "min": "1.80rem",
                        "max": "2.67rem"
                    },
					"size": "2.235rem",
					"slug": "large"
				},
				{
					"name": "Extra Large",
                    "fluid": {
                        "min": "2.16rem",
                        "max": "3.55rem"
                    },
					"size": "2.855rem",
					"slug": "x-large"
				},
                {
					"name": "Huge",
                    "fluid": {
                        "min": "2.59rem",
                        "max": "4.74rem"
                    },
					"size": "3.665rem",
					"slug": "huge"
				},
                {
					"name": "Gigantic",
                    "fluid": {
                        "min": "3.11rem",
                        "max": "6.31rem"
                    },
					"size": "4.725rem",
					"slug": "gigantic"
				}
            ],
            "fontFamilies": [
                {
					"name": "Roboto Slab",
                    "fontFamily": "\"Roboto Slab\", serif",
					"slug": "heading-slab",
                    "fontFace": [
                        {
                            "fontFamily": "\"Roboto Slab\"",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "400",
                            "src": [
                                "file:./dist/fonts/roboto-slab-v25-latin-regular.woff2"
                            ]
                        },
                        {
                            "fontFamily": "\"Roboto Slab\"",
                            "fontStretch": "normal",
                            "fontStyle": "normal",
                            "fontWeight": "700",
                            "src": [
                                "file:./dist/fonts/roboto-slab-v25-latin-700.woff2"
                            ]
                        }
                    ]
                },
                {
                    "fontFamily": "system-ui, sans-serif",
                    "name": "System UI",
                    "slug": "system-ui"
                },
                {
                    "fontFamily": "Charter, 'Bitstream Charter', 'Sitka Text', Cambria, serif",
                    "name": "Transitional",
                    "slug": "transitional"
                },
                {
                    "fontFamily": "'Iowan Old Style', 'Palatino Linotype', 'URW Palladio L', P052, serif",
                    "name": "Old Style",
                    "slug": "old-style"
                },
                {
                    "fontFamily": "Seravek, 'Gill Sans Nova', Ubuntu, Calibri, 'DejaVu Sans', source-sans-pro, sans-serif",
                    "name": "Humanist",
                    "slug": "humanist"
                },
                {
                    "fontFamily": "Avenir, Montserrat, Corbel, 'URW Gothic', source-sans-pro, sans-serif",
                    "name": "Geometric Humanist",
                    "slug": "geometric-humanist"
                },
                {
                    "fontFamily": "Optima, Candara, 'Noto Sans', source-sans-pro, sans-serif",
                    "name": "Classical Humanist",
                    "slug": "classical-humanist"
                },
                {
                    "fontFamily": "Inter, Roboto, 'Helvetica Neue', 'Arial Nova', 'Nimbus Sans', Arial, sans-serif",
                    "name": "Neo-Grotesque",
                    "slug": "neo-grotesque"
                },
                {
                    "fontFamily": "'Nimbus Mono PS', 'Courier New', monospace",
                    "name": "Monospace Slab Serif",
                    "slug": "monospace-slab-serif"
                },
                {
                    "fontFamily": "ui-monospace, 'Cascadia Code', 'Source Code Pro', Menlo, Consolas, 'DejaVu Sans Mono', monospace",
                    "name": "Monospace Code",
                    "slug": "monospace-code"
                },
                {
                    "fontFamily": "Bahnschrift, 'DIN Alternate', 'Franklin Gothic Medium', 'Nimbus Sans Narrow', sans-serif-condensed, sans-serif",
                    "name": "Industrial",
                    "slug": "industrial"
                },
                {
                    "fontFamily": "ui-rounded, 'Hiragino Maru Gothic ProN', Quicksand, Comfortaa, Manjari, 'Arial Rounded MT', 'Arial Rounded MT Bold', Calibri, source-sans-pro, sans-serif",
                    "name": "Rounded Sans",
                    "slug": "rounded-sans"
                },
                {
                    "fontFamily": "Rockwell, 'Rockwell Nova', 'Roboto Slab', 'DejaVu Serif', 'Sitka Small', serif",
                    "name": "Slab Serif",
                    "slug": "slab-serif"
                },
                {
                    "fontFamily": "Superclarendon, 'Bookman Old Style', 'URW Bookman', 'URW Bookman L', 'Georgia Pro', Georgia, serif",
                    "name": "Antique",
                    "slug": "antique"
                },
                {
                    "fontFamily": "Didot, 'Bodoni MT', 'Noto Serif Display', 'URW Palladio L', P052, Sylfaen, serif",
                    "name": "Didone",
                    "slug": "didone"
                },
                {
                    "fontFamily": "'Segoe Print', 'Bradley Hand', Chilanka, TSCu_Comic, casual, cursive",
                    "name": "Handwritten",
                    "slug": "handwritten"
                }
            ]
        }
    },
    "styles": {
        "blocks": {
            "core/avatar": {
                "border": {
                    "radius": "20rem"
                }
            },
            "core/post-author-name": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-condensed)",
                    "fontSize": "var(--wp--preset--font-size--x-large)"
                }
            },
            "core/button": {
                "spacing": {
                    "padding": {
                        "top": "var(--space-2xs-xs)",
                        "right": "var(--space-s-m)",
                        "bottom": "var(--space-2xs-xs)",
                        "left": "var(--space-s-m)"
                    }
                }
            },
            "core/cover": {
                "spacing": {
                    "padding": {
                        "top": "var(--space-s-m)",
                        "right": "var(--space-s-m)",
                        "bottom": "var(--space-s-m)",
                        "left": "var(--space-s-m)"
                    }
                }
            },
            "core/image": {
                "elements": {
                    "caption": {
                        "color": {
                            "text": "var(--wp--preset--color--background)",
                            "background": "var(--wp--preset--color--greyscale-quinary)"
                        },
                        "spacing": {
                            "margin": {
                                "top": "0"
                            },
                            "padding": {
                                "top": "var(--space-2xs-xs)",
                                "right": "var(--space-2xs-xs)",
                                "bottom": "var(--space-2xs-xs)",
                                "left": "var(--space-2xs-xs)"
                            }
                        },
                        "typography": {
                            "fontSize": "var(--wp--preset--font-size--small) !important"
                        }
                    }
                }
            },
            "core/navigation": {
				"elements": {
					"link": {
						":hover": {
                            "color": {
                                "background": "var(--wp--preset--color--primary)",
                                "text": "var(--wp--preset--color--background) !important"
                            }
						},
						":focus": {
							"typography": {
								"textDecoration": "underline"
							}
						},
						":active": {
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
                "spacing": {
                    "blockGap": "var(--space-4xs-3xs)"
                },
                "typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
                    "lineHeight": "var(--wp--custom--line-height--small)"
				}
            },
            "core/navigation-link": {
                "elements": {
                    "link": {
                        ":hover": {
                            "color": {
                                "background": "var(--wp--preset--color--primary-dark)",
                                "text": "var(--wp--preset--color--background) !important"
                            }
                        },
                        ":focus": {
                            "typography": {
                                "textDecoration": "underline"
                            }
                        },
                        ":active": {
                            "typography": {
                                "textDecoration": "none"
                            }
                        },
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                },
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-slab)"
                }
            },
            "core/navigation-submenu": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-slab)",
                    "lineHeight": "var(--wp--custom--line-height--base)"
                }
            },
            "core/post-author": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-slab)",
                    "lineHeight": "var(--wp--custom--line-height--tiny)"
                }
            },
            "core/post-date": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-slab)",
                    "fontWeight": "var(--wp--custom--font-weight--base)",
                    "lineHeight": "var(--wp--custom--line-height--base)"
                }
            },
            "core/post-excerpt": {
				"elements": {
					"link": {
                        "border": {
                            "radius": "var(--wp--custom--border--radius--sm)"
                        },
						"color": {
				            "background": "var(--wp--preset--color--link-primary)",
					        "text": "var(--wp--preset--color--background)"
						},
                        ":hover": {
                            "color": {
                                "background": "var(--wp--preset--color--link-secondary)",
                                "text": "var(--wp--preset--color--background)"
                            }
                        },
                        ":focus": {
                            "color": {
                                "background": "var(--wp--preset--color--link-secondary)",
                                "text": "var(--wp--preset--color--background)"
                            }
                        },
                        ":active": {
                            "color": {
                                "background": "var(--wp--preset--color--link-secondary)",
                                "text": "var(--wp--preset--color--background)"
                            }
                        },
						"spacing": {
                            "padding": {
                                "top": "var(--space-2xs-xs)",
                                "right": "var(--space-s-m)",
                                "bottom": "var(--space-2xs-xs)",
                                "left": "var(--space-s-m)"
                            }
                        },
                        "typography": {
                            "fontWeight": "var(--wp--custom--font-weight--medium)",
                            "fontSize": "var(--wp--preset--font-size--normal)",
                            "lineHeight": "var(--wp--custom--line-height--small)",
                            "textDecoration": "none"
                        }
					}
				},
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "lineHeight": "var(--wp--custom--line-height--medium)"
                }
			},
            "core/post-navigation-link": {
                "typography": {
                  "fontSize": "var(--wp--preset--font-size--small)",
                  "fontWeight": "var(--wp--custom--font-weight--medium)"
                }
              },
            "core/post-terms": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "var(--wp--custom--font-weight--medium)",
                    "textTransform": "capitalize"
                }
            },
            "core/pullquote": {
                "color": {
                    "background": "var(--wp--preset--color--greyscale-primary)"
                },
                "elements": {
                    "cite": {
                        "typography": {
                            "fontSize": "var(--wp--preset--font-size--small)",
                            "fontStyle": "normal",
                            "fontWeight": "var(--wp--custom--font-weight--medium)"
                        }
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "lineHeight": "var(--wp--custom--line-height--medium)"
                }
            },
            "core/query-pagination": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontWeight": "var(--wp--custom--font-weight--medium)"
                  }
            },
            "core/quote": {
                "border": {
                    "style": "solid",
                    "color": "var(--wp--preset--color--greyscale-quaternary)",
                    "width": "0 0 0 5px"
                },
                "color": {
                    "text": "var(--wp--preset--color--greyscale-quinary)",
                    "background": "var(--wp--preset--color--greyscale-primary)"
                },
                "elements": {
                    "cite": {
                        "typography": {
                            "fontSize": "var(--wp--preset--font-size--small)",
                            "fontStyle": "normal",
                            "fontWeight": "var(--wp--custom--font-weight--medium)"
                        }
                    }
                },
                "spacing": {
                    "padding": {
                        "top": "var(--space-s-m)",
                        "right": "var(--space-m-l)",
                        "bottom": "var(--space-s-m)",
                        "left": "var(--space-m-l)"
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--medium)",
                    "lineHeight": "var(--wp--custom--line-height--medium)"
                }
            },
            "core/search": {
                "color": {
                    "background": "var(--wp--preset--color--greyscale-primary)"
                },
                "spacing": {
                    "padding": {
                        "top": "var(--space-xs-s)",
                        "right": "var(--space-xs-s)",
                        "bottom": "var(--space-xs-s)",
                        "left": "var(--space-xs-s)"
                    }
                }
            },
            "core/site-title": {
                "elements": {
                    "link": {
                        "typography": {
                            "textDecoration": "none"
                        }
                    }
                },
                "typography": {
                    "lineHeight": "var(--wp--custom--line-height--base)"
                }
            }
        },
        "border": {},
        "color": {
            "text": "var(--wp--preset--color--foreground)",
            "background": "var(--wp--preset--color--background)"
        },
        "elements": {
            "button": {
				"border": {
					"radius": "var(--wp--custom--border--radius--sm)"
				},
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--background)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--secondary-dark)",
						"text": "var(--wp--preset--color--background)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--secondary-dark)",
						"text": "var(--wp--preset--color--background)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--secondary)",
						"text": "var(--wp--preset--color--background)"
					}
				},
                "typography": {
                    "fontWeight": "var(--wp--custom--font-weight--heavy)"
                }
			},
            "link": {
                "color": {
                    "background": "transparent",
                    "text": "var(--wp--preset--color--link-primary)"
                },
                ":hover": {
                    "color": {
						"text": "var(--wp--preset--color--link-secondary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
                    "color": {
						"text": "var(--wp--preset--color--link-secondary)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--link-secondary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
                "typography": {
					"textDecoration": "underline"
				}
            },
            "heading": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--heading-slab)",
                    "lineHeight": "var(--wp--custom--line-height--base)"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--huge)",
                    "fontWeight": "var(--wp--custom--font-weight--medium)",
                    "lineHeight": "var(--wp--custom--line-height--large)"
                }
            },
            "h2": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--x-large)",
                    "fontWeight": "var(--wp--custom--font-weight--medium)"
                }
            },
            "h3": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--large)",
                    "fontWeight": "var(--wp--custom--font-weight--base)"
                }
            },
            "h4": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "var(--wp--custom--font-weight--base)"
                }
            },
            "h5": {
                "typography": {
				    "fontSize": "var(--wp--preset--font-size--medium)",
                    "fontWeight": "var(--wp--custom--font-weight--base)"
                },
                "color": {
                    "text": "var(--wp--preset--color--greyscale-quinary)"
                }
            },
            "h6": {
                "typography": {
